{
    "name": "adambranscum/laravel-form-builder",
    "description": "A powerful Laravel package for creating dynamic forms with unique URLs, email notifications, and analytics",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "form",
        "form-builder",
        "dynamic-forms",
        "survey",
        "analytics"
    ],
    "authors": [
        {
            "name": "Adam Branscum",
            "email": "adam.branscum@icloud.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "YourUsername\\FormBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourUsername\\FormBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "adambranscum\\FormBuilder\\FormBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}